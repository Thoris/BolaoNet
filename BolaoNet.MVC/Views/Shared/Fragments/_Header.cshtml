{
<div class="row wrapper border-bottom white-bg page-heading">



    <div class="col-lg-10">
        @*<h2>@ViewBag.Grupo</h2>
        @Html.MvcSiteMap().SiteMapPath("BootstrapSiteMapPathHelperModel")*@

        @*<h2>Grupo...</h2>
        TESTE*@


        <h2>@ViewBag.Title</h2>        
