@model BolaoNet.MVC.ViewModels.LogReporting.LogReportingViewModel
@{
    ViewBag.Title = "Visualização de Logs";
} 



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">

        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h3>Logs</h3>
                </div>
                <div class="ibox-content" style="display: block;">

                    <div class="row">
                        <div class="col-xs-2">
                            <div class="form-group">

                                

                            </div>
                        </div>
                    </div>

                    <div class="row">
                        @if (Model.List != null && Model.List.Count > 0)
                        {
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <td></td>
                                        <td>@Html.DisplayNameFor(model => model.List[0].Id)</td> 
                                        <td></td>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int c = 0; c < Model.List.Count; c++)
                                    {
                                        <tr>
                                            <td></td>
                                            <td>@Html.DisplayFor(model => model.List[c].Id)</td>                                            
                                            <td></td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        }
                        else
                        {
                            <h3>Não foram encontrados logs</h3>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




 