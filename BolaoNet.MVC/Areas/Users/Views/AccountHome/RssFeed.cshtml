@model List<BolaoNet.Feed.Rss.EntryFeedItem>
@{
    ViewBag.Title = "Feed de Notícias do BolãoNet";
}
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">

            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h3>RSS</h3>
                    </div>
                    <div class="ibox-content" style="display: block;">

                        <div class="row">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <td>Id</td>
                                        <td>CreatedBy</td>
                                        <td>DateAdded</td>
                                        <td>Title</td>
                                        <td>Description</td>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int c = 0; c < Model.Count; c++)
                                    {
                                        <tr>
                                            <td>@Model[c].Id</td>
                                            <td>@Model[c].CreatedBy</td>
                                            <td>@Model[c].DateAdded</td>
                                            <td>@Model[c].Title</td>
                                            <td>@Model[c].Description</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}