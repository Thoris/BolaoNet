<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>SB Admin 2 - Bootstrap Admin Theme</title>



    <!-- Bootstrap Core CSS -->
    @*@Styles.Render("~/bundles/bootstrapcss")*@
    <link href="~/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- MetisMenu CSS -->
    @*@Styles.Render("~/bundles/metisMenucss")*@
    <link href="~/Content/vendor/metisMenu/metisMenu.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    @*@Styles.Render("~/bundles/sitecss")*@
    <link href="~/Content/css/sb-admin-2.css" rel="stylesheet">
    <!-- Morris Charts CSS -->
    <link href="~/Content/vendor/morrisjs/morris.css" rel="stylesheet">
    @*@Styles.Render("~/bundles/morriscss")*@
    <!-- Custom Fonts -->
    <link href="~/Content/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    @*@Styles.Render("~/bundles/font-awesome")*@
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    @RenderSection("header", required: false)


    @if (!string.IsNullOrEmpty(System.Configuration.ConfigurationManager.AppSettings["GoogleAnalyticsId"]))
    {
        @Html.Partial("_GoogleAnalytics")
    }

</head>
<body>
    
    @RenderBody()

    <!-- jQuery -->
    <script src="~/Content/vendor/jquery/jquery.min.js"></script>
    @*@Scripts.Render("~/bundles/jquery")*@
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Content/vendor/bootstrap/js/bootstrap.min.js"></script>
    @*@Scripts.Render("~/bundles/bootstrapjs")*@
    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Content/vendor/metisMenu/metisMenu.min.js"></script>
    @*@Scripts.Render("~/bundles/metisMenujs")*@
    <!-- Morris Charts JavaScript -->
    <script src="~/Content/vendor/raphael/raphael.min.js"></script>
    <script src="~/Content/vendor/morrisjs/morris.min.js"></script>
    <script src="~/Content/data/morris-data.js"></script>
    @*<!--@Scripts.Render("~/bundles/morrisjs") -->*@
    <!-- Custom Theme JavaScript -->
    <script src="~/Content/js/sb-admin-2.js"></script>
    @*@Scripts.Render("~/bundles/sitejs")*@

    @RenderSection("scripts", required: false)
</body>
</html>
